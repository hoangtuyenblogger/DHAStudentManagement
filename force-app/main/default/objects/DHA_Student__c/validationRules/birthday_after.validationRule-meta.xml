<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>birthday_after</fullName>
    <active>false</active>
    <description>Student birthdate should be after 1990/01/01</description>
    <errorConditionFormula>Birthday__c &lt; date(1990,01,01)</errorConditionFormula>
    <errorMessage>This student birthdate is invalid. Birthdate should be after 1990/01/01</errorMessage>
</ValidationRule>
